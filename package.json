{
  "name": "@davide94/llm-provider",
  "version": "0.1.2",
  "description": "Unified LLM provider abstraction with Langfuse tracing",
  "repository": {
    "type": "git",
    "url": "https://github.com/davide94/LLMProvider.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "release": "npm run build && npm version patch && npm publish",
    "release:minor": "npm run build && npm version minor && npm publish",
    "release:major": "npm run build && npm version major && npm publish"
  },
  "keywords": [
    "llm",
    "openai",
    "anthropic",
    "gemini",
    "langfuse",
    "ai",
    "tracing"
  ],
  "author": "Davide Rossetto",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.70.1",
    "@google/generative-ai": "^0.24.1",
    "@langfuse/client": "^4.4.2",
    "@langfuse/openai": "^4.4.2",
    "openai": "^6.9.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18"
  }
}
